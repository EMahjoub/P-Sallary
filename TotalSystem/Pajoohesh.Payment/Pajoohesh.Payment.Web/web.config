  <log4net>
    <appender name="RollingPatternFileAppender" type="log4net.Appender.RollingFileAppender">
      <param name="File" value="ClientLogs\LogPajoohesh.TotalSystem.${COMPUTERNAME}." />
      <encoding value="utf-8" />
      <param name="AppendToFile" value="false" />
      <param name="RollingStyle" value="Date" />
      <datePattern value=".yyyy.MM.dd_hh.mm.ss.fff.lo\g" />
      <param name="StaticLogFileName" value="false" />
      <layout type="log4net.Layout.PatternLayout">
        <header value="[Header] " />
        <footer value="[Footer] " />
        <conversionPattern value="%date %-5level %logger ${COMPUTERNAME} - %message%newline" />
      </layout>
    </appender>
    <logger name="SPM.SERVER">
      <level value="All" />
      <appender-ref ref="RollingPatternFileAppender" />
    </logger>
  </log4net>

